help:
	@echo "Targets disponibles:"
	@echo "  lint"
	@echo "  build"
	@echo "  unit-tests"
	@echo "  validate"
	@echo "  integration-tests"

lint:
	@echo "Running lint..."
	@echo "Lint OK"

build:
	@echo "Building project..."
	@echo "Build OK"

unit-tests:
	@echo "Running unit tests..."
	@echo "Unit tests OK"

validate:
	@echo "Validating project..."
	@echo "Validation OK"

integration-tests:
	@echo "Running integration tests..."
	@echo "Integration tests OK"

.DEFAULT_GOAL := help
